<?xml version="1.0"?>

<launch>
  <let name="layer" value="elevation"/>
  <let name="grid_map_topic" value="/elevation_mapping/elevation_map"/>

  <node namespace="hector_grid_map_compression" name="server" exec="hector_grid_map_compression_server" pkg="hector_grid_map_compression" output="screen">
    <remap from="input" to="$(var grid_map_topic)"/>
    <remap from="output" to="compressed"/>
    <param name="layers" value="[$(var layer)]"/>
  </node>
</launch>
