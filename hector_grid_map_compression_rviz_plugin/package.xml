  <exec_depend>grid_map_ros</exec_depend>
  <exec_depend>grid_map_msgs</exec_depend>
  <exec_depend>hector_grid_map_compression</exec_depend>
  <exec_depend>hector_grid_map_compression_msgs</exec_depend>
